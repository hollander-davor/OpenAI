{
    "name": "davor/open-ai",
    "description": "Package for communication with OpenAI",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hoks\\OpenAi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Davor Holender",
            "email": "davor.holender@cubes.rs"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0.0",
        "intervention/image": "^2.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers":[
                "Hoks\\OpenAI\\OpenAIServiceProvider"
            ],
            "aliases":{
                "OpenAI" : "Hoks\\OpenAI\\Classes\\Facades\\OpenAI"
            }
        }
    }
}
